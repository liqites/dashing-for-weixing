<% content_for :title do %>My super sweet dashboard<% end %>
<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="welcome" data-view="Title" data-title="开口平衡表" data-button="退出全屏"></div>
    </li>
    <li data-row="1" data-col="1" data-sizex="3" data-sizey="1">
      <div data-id="metalclose_output" data-view="Graph" data-title="今日实际产量与前一个月实际产量" data-graphtype="area"></div>
    </li>
    <li data-row="1" data-col="1" data-sizex="4" data-sizey="2">
      <div data-id="metalclose_capicity" data-view="Highchart" data-title="前一个月缺数、完工数、核定产能对比" data-graphtype="column"></div>
    </li>
    <li data-row="2" data-col="1" data-sizex="2" data-sizey="2">
      <div data-id="buzzwords" data-view="List" data-unordered="true" data-title="缺失详细" ></div>
    </li>
    <!--<li data-row="4" data-col="1" data-sizex="2" data-sizey="1">-->
      <!--<div data-id="valuation" data-view="Number" data-title="今日产量" data-suffix=""></div>-->
    <!--</li>-->

    <li data-row="5" data-col="3" data-sizex="2" data-sizey="1">
      <div data-id="synergy" data-view="Meter" data-title="前一个月的实际产量周日均与实际产量" data-min="0" data-max="100"></div>
    </li>
    <li data-row="5" data-col="3" data-sizex="1" data-sizey="1">
      <div  data-id="metalclose_sum_unfinished" data-view="Text" data-title="累计误期" ></div>
    </li>
    <li data-row="5" data-col="3" data-sizex="1" data-sizey="1">
      <div  data-id="metalclose_sum_planunfinished" data-view="Text" data-title="存量" ></div>
    </li>
  </ul>

</div>